<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - 管理平台</title>

    <meta name="description" content="overview &amp; stats" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- basic styles -->
    <link href="/Areas/Admin/Content/Default/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Areas/Admin/Content/Default/css/font-awesome.min.css" rel="stylesheet" />

    <!--[if IE 7]>
	<link href="/Areas/Admin/Content/Default/css/font-awesome-ie7.min.css" rel="stylesheet" />
	<![endif]-->

    <!-- page specific plugin styles -->
    <!-- fonts -->
    <link href="/Areas/Admin/Content/Default/css/googlefonts/css.css" rel="stylesheet" />
    <!-- ace styles -->

    <link href="/Areas/Admin/Content/Default/css/ace.min.css" rel="stylesheet" />
    <link href="/Areas/Admin/Content/Default/css/ace-rtl.min.css" rel="stylesheet" />
    <link href="/Areas/Admin/Content/Default/css/ace-skins.min.css" rel="stylesheet" />

    <!--[if lte IE 8]>
    <link href="/Areas/Admin/Content/Default/css/ace-ie.min.css" rel="stylesheet" />
	<![endif]-->

    <!-- inline styles related to this page -->
    <!-- ace settings handler -->
    <script src="/Areas/Admin/Content/Default/js/ace-extra.min.js"></script>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->

    <!--[if lt IE 9]>
        <script src="/Areas/Admin/Content/Default/js/html5shiv.js"></script>    
        <script src="/Areas/Admin/Content/Default/js/respond.min.js"></script>
	<![endif]-->


</head>
<body>
    @Html.Partial("_HeadPartial")
    <div class="main-container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>

        <div class="main-container-inner">


            @Html.Partial("_NavigationPartial")

            <div class="main-content">
                <div class="breadcrumbs" id="breadcrumbs">
                    <script type="text/javascript">
                        try { ace.settings.check('breadcrumbs', 'fixed') } catch (e) { }
                    </script>
                    <ul class="breadcrumb">
                        <li>
                            <i class="icon-home home-icon"></i>
                            <a href="@Url.Action("index","home")">管理后台</a>
                        </li>
                        @RenderSection("bread", false)
                    </ul>
                    <!-- .breadcrumb -->

                    <div class="nav-search" id="nav-search">
                        <form class="form-search">
                            <span class="input-icon">
                                <input type="text" placeholder="搜索 ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
                                <i class="icon-search nav-search-icon"></i>
                            </span>
                        </form>
                    </div>
                    <!-- #nav-search -->
                </div>
                @RenderBody()
                <div class="ace-settings-container" id="ace-settings-container">
                    <div class="btn btn-app btn-xs btn-warning ace-settings-btn" id="ace-settings-btn">
                        <i class="icon-cog bigger-150"></i>
                    </div>

                    <div class="ace-settings-box" id="ace-settings-box">
                        <div>
                            <div class="pull-left">
                                <select id="skin-colorpicker" class="hide">
                                    <option data-skin="default" value="#438EB9">#438EB9</option>
                                    <option data-skin="skin-1" value="#222A2D">#222A2D</option>
                                    <option data-skin="skin-2" value="#C6487E">#C6487E</option>
                                    <option data-skin="skin-3" value="#D0D0D0">#D0D0D0</option>
                                </select>
                            </div>
                            <span>&nbsp; Choose Skin</span>
                        </div>

                        <div>
                            <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-navbar" />
                            <label class="lbl" for="ace-settings-navbar">Fixed Navbar</label>
                        </div>

                        <div>
                            <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-sidebar" />
                            <label class="lbl" for="ace-settings-sidebar">Fixed Sidebar</label>
                        </div>

                        <div>
                            <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-breadcrumbs" />
                            <label class="lbl" for="ace-settings-breadcrumbs">Fixed Breadcrumbs</label>
                        </div>

                        <div>
                            <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-rtl" />
                            <label class="lbl" for="ace-settings-rtl">Right To Left (rtl)</label>
                        </div>

                        <div>
                            <input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-add-container" />
                            <label class="lbl" for="ace-settings-add-container">
                                Inside
								<b>.container</b>
                            </label>
                        </div>
                    </div>
                </div>
                @* <div class="page-content">
                    <div class="row">

                        <div class="alert alert-block alert-success">
                            本次页面执行时间2: 0 毫秒
                        </div>

                    </div>
                </div>*@

            </div>
        </div>

        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="icon-double-angle-up icon-only bigger-110"></i>
        </a>
    </div>
    <script src="/Areas/Admin/Content/Default/js/jquery-1.10.2.min.js"></script>
    @*<!--[if !IE]> -->
    <script src="/Areas/Admin/Content/Default/js/jquery-2.0.3.min.js"></script>
    <!-- <![endif]-->

    <!--[if IE]>
    <script src="/Areas/Admin/Content/Default/js/jquery-1.10.2.min.js"></script>
    <![endif]-->*@
    <script src="/Areas/Admin/Content/Default/js/bootstrap.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/bootbox.min.js"></script>
    
    <script src="/Areas/Admin/Content/Default/js/jquery.validate.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/jquery.validate.unobtrusive.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/jquery.unobtrusive-ajax.min.js"></script>
    <script src="/Areas/Admin/Scripts/Common.js"></script>
    <script type="text/javascript">
        if ("ontouchend" in document) document.write("<script src='/Areas/Admin/Content/Default/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
    </script>



    @*<script src="/Areas/Admin/Content/Default/js/typeahead-bs2.min.js"></script>*@
    <!-- page specific plugin scripts -->

    <!--[if lte IE 8]>
    <script src="/Areas/Admin/Content/Default/js/excanvas.min.js"></script>
	<![endif]-->
    @*    <script src="/Areas/Admin/Content/Default/js/jquery-ui-1.10.3.custom.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/jquery.ui.touch-punch.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/jquery.slimscroll.min.js"></script>

    <script src="/Areas/Admin/Content/Default/js/jquery.sparkline.min.js"></script>*@

    @*    <script src="/Areas/Admin/Content/Default/js/jquery.dataTables.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/jquery.dataTables.bootstrap.js"></script>*@

    <script src="/Areas/Admin/Content/Default/js/ace-elements.min.js"></script>
    <script src="/Areas/Admin/Content/Default/js/ace.min.js"></script>

    @RenderSection("scripts", false)
</body>
</html>
